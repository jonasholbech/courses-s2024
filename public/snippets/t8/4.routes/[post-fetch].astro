---
export async function getStaticPaths() {
  const data = await fetch("...").then((response) => response.json());

  return data.map((onePost) => {
    return {
      params: { id: onePost.id },
      props: { post: onePost },
    };
  });
}

//const { id } = Astro.params;
const { post } = Astro.props;
---

<h1>{post.name}</h1>
